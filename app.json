{
  "expo": {
    "name": "ConvertOne",
    "slug": "ConvertOne",
    "version": "1.2.4",
    "orientation": "portrait",
    "icon": "./assets/images/TheIcon.png",
    "scheme": "appconvertisseur",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.ethan.universel",
      "supportsTablet": true,
      "infoPlist": {
        "GADApplicationIdentifier": "ca-app-pub-6942695227282452~1099626887",
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads."
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6942695227282452~1099626887"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/TheIcon.png",
        "backgroundImage": "./assets/images/TheIcon.png",
        "monochromeImage": "./assets/images/TheIcon.png"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-6942695227282452~1099626887"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ethan.universel"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/TheIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TheIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7b05c3b1-0b06-4377-b2ed-9fa78ac4d773"
      }
    },
    "owner": "darstein_ethan"
  }
}
